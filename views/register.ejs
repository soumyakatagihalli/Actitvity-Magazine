<% try { %>
    <%- include('partials/header', {user: [firstName, lastName]}) %>
  <%} catch(error) { %>
    <%- include('partials/header')%>
  <%} %>
  <style>
    .container{
        background-color: whitesmoke;
    }
    .img-fluid{
        height:375px;
  
  
    }
  </style>
  <header>
    
    <div class="topnav" style="background-color: grey;">
      <a href="/" class="active" style="background-color: rgb(52, 49, 49);"><b>THE ASSOCIATION OF COMPUTER SCIENCE AND ENGINEERING</b></a>
      <a href="/" >Home</a>
      <a href="/history" >History</a>
      <a href="/" >Campus life</a>
      <a href="/" >Circulars</a>
      <a href="/" >Contact us</a>
      <div class="topnav-right">
        <form class="search-bar" action="/search" method="post">
          <input type="text" class="form-control" id="usr" placeholder="Search">
        </form>
        
          
          <a href="/login">Login</a>
          <a style="padding-right: 2rem;" href="/register">Register</a>
        


        
      </div>
    </div>
  </header>
<section class="vh-100">
    <div class="container">
        <div class="row d-flex align-items-center justify-content-center h-100">
            <h1 class="login-header">Register</h1>
            <div class="col-md-8 col-lg-7 col-xl-6">
                <img src="https://www.kletech.ac.in/wp-content/uploads/2020/02/new-banner.jpg"
                    class="img-fluid" alt="Phone image">
            </div>
            <div class="col-md-7 col-lg-5 col-xl-5 offset-xl-1">
                <form action="/register" method="post">
                    <!-- First Name input-->
                    <div class="row mb-4">
                        <div class="col">
                          <div class="form-outline">
                            <input name="firstName" type="text" id="form6Example" class="form-control form-control-lg" placeholder="First name"/>
                            <label class="form-label" for="form6Example1"></label>
                          </div>
                        </div>
                    <!-- Last Name input-->
                        <div class="col">
                          <div class="form-outline">
                            <input name="lastName" type="text" id="form6Example2" class="form-control form-control-lg"" placeholder="Last name" />
                            <label class="form-label" for="form6Example2"></label>
                          </div>
                        </div>
                      </div>
                    <!-- Email input -->
                    <div class="form-outline mb-4">
                        <input type="email" id="form1Example13" class="form-control form-control-lg"
                            placeholder="Email address" name="username" />
                        <label class="form-label" for="form1Example13"></label>
                    </div>

                    <!-- Password input -->
                    <div class="form-outline mb-4">
                        <input type="password" id="form1Example23" class="form-control form-control-lg"
                            placeholder="Password" name="password"/>
                        <label class="form-label" for="form1Example23"></label>
                    </div>

                    <div class="d-flex justify-content-around align-items-center mb-4">
                        <!-- Checkbox -->
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox" value="" id="form1Example3" checked />
                            <label class="form-check-label" for="form1Example3"> Remember me </label>
                        </div>
                        
                    </div>

                    <!-- Submit button -->
                    <button type="submit" style="width: 100%" class="btn btn-primary btn-lg btn-block">Sign Up</button>
                </form>
                    <div class="divider d-flex align-items-center my-4">
                        <p class="text-center fw-bold mx-3 mb-0 text-muted">OR</p>
                    </div>
                    <div class="container">
                        <div class="row">
                            <div class="col">
                                <a class="btn btn-primary" style="background-color: #3b5998" href="#!"
                                    role="button">
                                    <i class="fab fa-facebook-f me-2"></i>Continue with Facebook
                                </a>
                            </div>

                            <div class="col">
                                <a class="btn btn-primary " style="background-color: #55acee" href="#!"
                                    role="button">
                                    <i class="fab fa-twitter me-2"></i>Continue with Twitter</a>
                            </div>

                            <div class="col">
                                <a class="btn btn-primary " style="background-color: #ec4c4c" href="#!"
                                    role="button">
                                    <i class="fab fa-google me-2"></i>Continue with Google</a>
                            </div>

                        </div>
                    </div>
               
            </div>
        </div>
    </div>
</section>







